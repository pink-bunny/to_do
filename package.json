{
  "name": "react-pages-boilerplate",
  "version": "2.0.0",
  "repository": "https://github.com/GarrisonD/react-pages-boilerplate",
  "author": "Ihor Dotsenko <ihor.dotsenko@rubygarage.org> (https://github.com/GarrisonD)",
  "license": "UNLICENSED",
  "scripts": {
    "start:dev": "run-p -l server webpack:dev",
    "server": "nodemon --watch build/server.js --exec 'node build/server.js'",
    "webpack:clean": "rimraf build",
    "webpack:dev": "run-s webpack:clean && run-p -l 'webpack:dev:* --watch'",
    "webpack:dev:server": "cross-env NODE_ENV=development webpack --color --config webpack.server.js",
    "webpack:dev:client": "cross-env NODE_ENV=development webpack --color --config webpack.client.js"
  },
  "devDependencies": {
    "@types/express": "^4.11.0",
    "@types/history": "^4.6.2",
    "@types/react": "^16.0.35",
    "@types/react-dom": "^16.0.3",
    "@types/react-redux": "^5.0.14",
    "@types/react-router-dom": "^4.2.3",
    "@types/react-router-redux": "^5.0.11",
    "@types/redux-logger": "^3.0.5",
    "@types/webpack-env": "^1.13.3",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.1.0",
    "css-loader": "^0.28.7",
    "eslint": "^4.6.1",
    "eslint-plugin-react": "^7.3.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.5.3",
    "nodemon": "^1.14.9",
    "npm-run-all": "^4.1.2",
    "react-hot-loader": "^3.0.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-logger": "^3.0.6",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.20.1",
    "typescript": "^2.6.2",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.8",
    "redux": "^3.7.2",
    "redux-multi": "^0.1.91",
    "redux-thunk": "^2.2.0"
  }
}
